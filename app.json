{
  "expo": {
    "name": "map-location",
    "slug": "map-location",
    "newArchEnabled": true,
    "version": "1.0.1",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "android": {
      "versionCode": 1011,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.map.location",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "082a619f-f815-4086-a376-b2631eb1f6e7"
      }
    }
  }
}
